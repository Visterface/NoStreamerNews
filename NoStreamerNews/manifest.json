{
    "manifest_version": 3,
    "name": "NoStreamerNews",
    "version": "1.1",
    "description": "Entfernt alle Artikel über Streamer oder Youtuber von Mein-MMO.de, damit man nicht ständig diesen Schund sehen muss.",
    
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },

    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["https://mein-mmo.de/*"]
        }
    ]
    
}